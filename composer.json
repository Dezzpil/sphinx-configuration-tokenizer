{
  "name": "ltd-beget/sphinx-configuration-tokenizer",
  "type": "library",
  "description": "tokenize sphinx configuration and that's all, folks.",
  "keywords": [
    "sphinx",
    "configuration",
    "tokenize",
    "tokens"
  ],
  "authors": [
    {
      "name": "Viskov Sergey",
      "homepage": "https://github.com/voksiv"
    }
  ],
  "config": {
    "github-protocols": ["https"]
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/Dezzpil/stringstream.git",
      "no-api": true
    }
  ],
  "require": {
    "php": ">=7.0",
    "ltd-beget/stringstream" : "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "^5.7",
    "tideways/profiler": "2.0.*"
  },
  "autoload": {
    "psr-0": {
      "LTDBeget\\sphinx": "src"
    }
  }
}

